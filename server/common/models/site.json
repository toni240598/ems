  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "address": {
      "type": "string",
      "required": true
    },
    "district_id": {
      "type": "number",
      "required": true
    },
    "location": {
      "type": "geopoint",
      "required": true
    },
    "label": {
      "type": "string",
      "required": true
    },
    "zoom": {
      "type": "number",
      "required": true
    },
    "discobj_id": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
