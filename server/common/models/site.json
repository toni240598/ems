      "foreignKey": "district_id"
    },
    "managedObjects": {
      "type": "hasMany",
      "model": "managed_object",
      "foreignKey": "site_id"
    },
    "securitySites": {
      "type": "hasMany",
      "model": "security_site",
      "foreignKey": "site_id"
    },
    "items": {
