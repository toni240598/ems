{
  "name": "district",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "city_id": {
      "type": "number",
      "required": true
    },
    "label": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "geopoint",
      "required": true
    },
    "zoom": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "city": {
      "type": "belongsTo",
      "model": "city",
      "foreignKey": "city_id"
    },
    "sites": {
      "type": "hasMany",
      "model": "site",
      "foreignKey": "district_id"
    }
  },
  "acls": [],
  "methods": {}
}
